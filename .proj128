function publish()
{
    cd "$PROJECT"
    local version="$(cat package.json | jq -r .version)"
    read -p "Version: ($version) " version2
    cat package.json | jq ". | setpath([ \"version\" ]; \"$version2\")" | tee package.json > /dev/null
    sed -i "s#version = \"*\".*#version = \"$version2\"#" bin/proj128
    git add bin/proj128 package.json
    git commit -a -m "$version2"
    git tag "$version2"
    git push
    git push --tags
    npm publish
}
